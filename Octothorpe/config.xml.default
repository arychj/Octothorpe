<config>
    <debug>True</debug>
    <logging>
        <time_format><![CDATA[%Y-%m-%d %H:%M:%S]]></time_format>
    </logging>
    <processing>
        <worker_threads>5</worker_threads>
    </processing>
    <database>
        <path>./octothorpe.sqlite3</path>
        <queries>
            <basepath>./Resources/Queries</basepath>
        </queries>
    </database>
    <services>
        <directories>
            <directory></directory>
        </directories>
    </services>
</config>
